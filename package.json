  },
  "dependencies": {
    "@material-ui/core": "^3.1.1",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "fs-filesystem": "^2.1.2",
    "lodash": "^4.17.11",
    "lokijs": "^1.5.5",
    "morgan": "^1.9.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-scripts": "^1.1.5",
